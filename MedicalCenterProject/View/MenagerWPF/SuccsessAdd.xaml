        </Button>
        <Button Content="" HorizontalAlignment="Left" Margin="458,114,0,0" VerticalAlignment="Top" Width="34" Height="35" RenderTransformOrigin="-0.741,-2.455" Click="Button_Click_5">
            <Button.Background>
                <ImageBrush ImageSource="JKP-Prostor-Sombor.jpg"/>
