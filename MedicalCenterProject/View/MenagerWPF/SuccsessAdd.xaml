        </Button>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="361" Margin="297,29,0,0" VerticalAlignment="Top" Width="205"/>
        <Image Margin="415,39,300,310" Source="logo (1).png" Stretch="Fill"/>
        <Image Margin="381,351,371,40" Source="home-button.png" Stretch="Fill" RenderTransformOrigin="0.609,0.351"/>
        <Button Content="" HorizontalAlignment="Left" Margin="307,351,0,0" VerticalAlignment="Top" Width="33" Height="28" Click="Button_Click_7" RenderTransformOrigin="0.103,2.786">
