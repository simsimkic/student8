<Window x:Class="MedicalCenterProject.View.DoctorWPF.MedicalRecord"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MedicalCenterProject.View.DoctorWPF"
        mc:Ignorable="d"
        Title="MedicalRecord" Height="525" Width="750">
    <Grid>
        <ListView Height="420" Width="694" Margin="25,50,23,253" Name="lvUsers">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Date of examination" Width="150" DisplayMemberBinding="{Binding Datum1}" />
                </GridView>
            </ListView.View>
        </ListView>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Medical history" VerticalAlignment="Top" Margin="25,20,0,0" Height="25" Width="141" FontFamily="Corbel" FontSize="20" FontWeight="Bold"/>

    </Grid>
</Window>
